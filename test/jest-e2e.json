{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/./src/libs/config": "<rootDir>/../libs/src/libs/config/src",
    "@app/./src/libs/config/(.*)": "<rootDir>/../libs/src/libs/config/src/$1",
    "@app/src/libs/config": "<rootDir>/../libs/src/libs/config/src",
    "@app/src/libs/config/(.*)": "<rootDir>/../libs/src/libs/config/src/$1"
  }
}